 <div class="container">
				<div class="row">
					<div class="col-md-8">
						<h2>Add Agency</h2>

						<form action="/api/agencies" method="POST" enctype="multiport/form-data">
							<div class="form-group required-field">
                                <br>
								<label for="agencyName">Name</label>
								<input type="text" class="form-control" id="agencyName" name="agencyName" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="agencyPhone">Phone Number</label>
								<input type="text" class="form-control" id="agencyPhone" name="agencyPhone" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="agencyAddress">Address</label>
								<input type="text" class="form-control" id="agencyAddress" name="agencyAddress" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="agencyCity">City</label>
								<input type="text" class="form-control" id="agencyCity" name="agencyCity" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="agencyState">State</label>
								<input type="text" class="form-control" id="agencyState" name="agencyState" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="agencyZip">Zip</label>
								<input type="number" class="form-control" id="agencyZip" name="agencyZip" required>
							</div><!-- End .form-group -->	

							<div class="form-group required-field">
                                <br>
								<label for="contactName">Contact Name</label>
								<input type="text" class="form-control" id="contactName" name="contactName" required>
							</div><!-- End .form-group -->

							<div class="form-group required-field">
                                <br>
								<label for="contactEmail">Contact Email</label>
								<input type="text" class="form-control" id="contactEmail" name="contactEmail" required>
							</div><!-- End .form-group -->							

							<div class="form-footer">
								<button type="submit" class="btn btn-primary">Submit</button>
							</div><!-- End .form-footer -->
						</form>
					</div><!-- End .col-md-8 -->
					
				</div><!-- End .row -->
			</div><!-- End .container -->

			
    <div class="mb-8"></div><!-- margin -->